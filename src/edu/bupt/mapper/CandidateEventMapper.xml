<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.bupt.mapper.CandidateEventMapper">
    <insert id="addEvent" parameterType="CandidateEvent" useGeneratedKeys="true" keyProperty="candidateEventId">
        insert into candidateevents values(#{candidateEventId},#{triggerWordId},#{sentenceId})
    </insert>
</mapper>


